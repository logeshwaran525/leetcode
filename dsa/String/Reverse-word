class Solution {
    public String reverseWords(String s) {
        String str = s.replaceAll("\\s+"," ");
        String ss[] = str.split(" ");
        String result = "";
        for(int i=ss.length-1 ; i>=0 ; i--)
        {
            result += ss[i] + " ";
        } 
        return result.trim();
    }
}
